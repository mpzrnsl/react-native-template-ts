{
  "name": "ProjectName",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:clean": "cd android && ./gradlew clean && cd ..",
    "android:staging": "react-native run-android --variant=staging",
    "android:staging:install": "cd android && ./gradlew installStaging && cd ..",
    "android:staging:bundle": "cd android && ./gradlew bundleStaging && cd ..",
    "android:staging:assemble": "cd android && ./gradlew assembleStaging && cd ..",
    "android:release": "react-native run-android --variant=release",
    "android:release:install": "cd android && ./gradlew installRelease && cd ..",
    "android:release:bundle": "cd android && ./gradlew bundleRelease && cd ..",
    "android:release:assemble": "cd android && ./gradlew assembleRelease && cd ..",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "@formatjs/intl-datetimeformat": "^6.3.1",
    "@formatjs/intl-displaynames": "^6.1.4",
    "@formatjs/intl-getcanonicallocales": "^2.0.4",
    "@formatjs/intl-listformat": "^7.1.3",
    "@formatjs/intl-locale": "^3.0.7",
    "@formatjs/intl-numberformat": "^8.2.0",
    "@formatjs/intl-pluralrules": "^5.1.4",
    "@formatjs/intl-relativetimeformat": "^11.1.4",
    "@hookform/resolvers": "^2.9.9",
    "@react-native-async-storage/async-storage": "^1.17.10",
    "@react-navigation/bottom-tabs": "^6.4.0",
    "@react-navigation/drawer": "^6.5.0",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/native-stack": "^6.9.1",
    "@reduxjs/toolkit": "^1.8.6",
    "@rneui/base": "^4.0.0-rc.6",
    "@rneui/themed": "^4.0.0-rc.6",
    "axios": "0.27.2",
    "i18next": "^21.10.0",
    "react": "18.1.0",
    "react-hook-form": "^7.37.0",
    "react-i18next": "^11.18.6",
    "react-native": "0.70.3",
    "react-native-config": "^1.4.11",
    "react-native-error-boundary": "^1.1.16",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "^2.7.1",
    "react-native-localize": "^2.2.4",
    "react-native-logs": "^5.0.1",
    "react-native-reanimated": "^2.11.0",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-screens": "^3.18.2",
    "react-native-vector-icons": "^9.2.0",
    "react-redux": "^8.0.4",
    "redux-persist": "^6.0.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@ianvs/prettier-plugin-sort-imports": "^3.4.2",
    "@react-native-community/eslint-config": "^2.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^26.0.23",
    "@types/react": "^18.0.21",
    "@types/react-native": "^0.70.4",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.32.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.1",
    "jest": "^26.6.3",
    "lint-staged": "^13.0.3",
    "metro-react-native-babel-preset": "0.72.3",
    "react-test-renderer": "18.1.0",
    "typescript": "^4.8.3"
  },
  "jest": {
    "preset": "react-native",
    "testMatch": [
      "**/tests/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npx prettier --write",
      "npx eslint --fix"
    ]
  }
}
